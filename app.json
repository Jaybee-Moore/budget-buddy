{
  "expo": {
    "name": "budget-buddy",
    "slug": "budget-buddy",
    "version": "1.0.0",
    "orientation": "portrait",
  "icon": "./assets/logo.png",
    "scheme": "budgetbuddy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
  "supportsTablet": true,
  "icon": "./assets/logo.png",
  "iconAssetPath": "./assets/logo.png",
  "iconBackgroundColor": "#FFFFFF",
  "iconPadding": true,
  "iconRoundedCorners": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
